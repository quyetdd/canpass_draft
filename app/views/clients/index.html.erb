<h1>Listing clients</h1>
<div>
<table border="1">
  <tr>
    <th>Romaji name</th>
  </tr>
  <% @clients.each do |client| %>
    <tr>
      <td><%= link_to client.romaji_name, client_path(client) %></td>
    </tr>
  <% end %>
</table>
</div>
<%= link_to 'New Client', new_client_path %>
<%= render "promotions/promotions" %>
