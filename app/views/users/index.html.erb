<h1>Listing users</h1>
<%= link_to 'New User', new_user_path %>
<table>
  <tr>
    <th>Username</th>
    <th>Role</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user_path(user) %></td>
      <td><%= Role.find(user.role_id).role_name %></td>
    </tr>
  <% end %>
</table>
